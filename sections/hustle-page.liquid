{% schema %}
{
  "name": "Hustle Page",
  "max_blocks": 5,
  "settings": [
    {
        "id": "heroTitle",
        "type": "text",
        "label": "Hero Title",
        "default": "Hustle"
    },
    {
        "id": "heroParagraph",
        "type": "text",
        "label": "Hero Paragraph",
        "default": "When the calling pulls you beyond your comfort zone. When you go for greatness and give it everything you’ve got. When you’re more than what they say you are and the world needs to know. This is hustle."
    },
    {
        "type": "checkbox",
        "id": "btn-enabled",
        "label": "Enabled Button"
    },
    {
        "id": "heroBtnLink",
        "type": "url",
        "label": "Hero Button URL"
    },
    {
        "id": "heroBtnText",
        "type": "text",
        "label": "Hero Button Text",
        "default": "Button Text"
    },
    {
        "id": "heroImage",
        "type": "image_picker",
        "label": "Hero Image"
    },
    {
        "id": "quote",
        "type": "text",
        "label": "Quote Content",
        "default": "Luke Skywalker should have joined the dark side and conquered the galaxy with Darth Vader."
    },
    {
        "id": "quoteName",
        "type": "text",
        "label": "Quote Name",
        "default": "Justin Forset"
    },
    {
        "id": "quoteCareer",
        "type": "text",
        "label": "Quote Career / Title",
        "default": "Founder - Hustle Clean"
    },
    {
        "id": "instaTitle",
        "type": "text",
        "label": "Instagram Title",
        "default": "This is a headline"
    },
    {
        "id": "instaSubtitle",
        "type": "text",
        "label": "Instagram Subtitle",
        "default": "Subheadline"
    },
    {
        "id": "instaBtnLink",
        "type": "url",
        "label": "Instagram Button Link"
    },
    {
        "id": "instaBtnText",
        "type": "text",
        "label": "Instagram Button Text",
        "default": "Follow Hustle Clean"
    }    
  ],
  "blocks": [
  {
    "type": "program",
    "name": "Program",
    "settings": [
      {
        "id": "title",
        "type": "text",
        "label": "Title",
        "default": "This Is A Headline Title"
      },
      {
        "id": "subtitle",
        "type": "text",
        "label": "Subtitle",
        "default": "We see you. You're hustling, you're grinding."
      },
      {
        "id": "paragraph",
        "type": "text",
        "label": "Paragraph",
        "default": "You’re taking kids to school, being a boss in your business, pushing your PB, getting that promotion. You are a high performer. So we’re not about to let you under perform when it comes to hygiene."
      },
      {
        "id": "listTitle",
        "type": "text",
        "label": "List Title",
        "default": "Use For"
      },
      {
        "id": "listItem-one",
        "type": "text",
        "label": "List Item",
        "default": "List Item"
      },
      {
        "id": "listItem-two",
        "type": "text",
        "label": "List Item",
        "default": "List Item"
      },
      {
        "id": "listItem-three",
        "type": "text",
        "label": "List Item",
        "default": "List Item"
      },
      {
        "id": "listItem-four",
        "type": "text",
        "label": "List Item",
        "default": "List Item"
      },
      {
        "id": "listItem-five",
        "type": "text",
        "label": "List Item",
        "default": "List Item"
      },
      {
        "id": "listItem-six",
        "type": "text",
        "label": "List Item",
        "default": "List Item"
      },
      {
        "id": "btnLink",
        "type": "url",
        "label": "Button URL"
      },
      {
        "id": "btnText",
        "type": "text",
        "label": "Button Text",
        "default": "Sign Me Up"
      },
      {
        "id": "image",
        "type": "image_picker",
        "label": "Image"
      }
    ]
  }
]
}
{% endschema %}
